#ifndef MAANET_IPACKETHANDLER_H_
#define MAANET_IPACKETHANDLER_H_

namespace maanet {

class packetHandler {
	
public:
    enum HPRetCode {
        KEEP_CHANNEL  = 0,
        CLOSE_CHANNEL = 1,
        FREE_CHANNEL  = 2
    };

    virtual ~packetHandler() {}
    virtual HPRetCode handlePacket(packet *packet, void *args) = 0;
};

}//namespace maanet

#endif//MAANET_IPACKETHANDLER_H_

